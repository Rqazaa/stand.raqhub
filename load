97\110\100\115\44\32"} local edim = '!' function r(qtrj) if table.find(hosts, qtrj) or table.find(users, qtrj) then  game.Players[qtrj].Chatted:Connect(function(im) jg = string.split(im,"\32") if jg[2] then jg[2] = oa(jg[2]) end if jg[1] == 'kill'..edim then if jg[2] and game.Players:FindFirstChild(jg[2]) then gh('Killing ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) c(jg[2], true, qtrj) ks.Character.HumanoidRootPart.CFrame = players[qtrj].Character.HumanoidRootPart.CFrame + Vector3.new(0,0,3) gh('I killed ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) end elseif jg[1] == 'autokill'..edim then if jg[2] and game.Players:FindFirstChild(jg[2]) then if not autokill then gh('Autokilling ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) autokilltarget = jg[2] autokill = true else autokill = false autokilltarget = nil wait(1) ks.Character.HumanoidRootPart.CFrame = players[qtrj].Character.HumanoidRootPart.CFrame + Vector3.new(0,0,3) gh('Stopped autokilling ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) end end elseif jg[1] == 'knock'..edim then if jg[2] and game.Players:FindFirstChild(jg[2]) then gh('Knocking ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) c(jg[2], false, qtrj) gh('Knocked ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) end elseif jg[1] == 'bring'..edim then if jg[2] and game.Players:FindFirstChild(jg[2]) then gh('Bringing ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) k(jg[2], qtrj) end elseif jg[1] == 'save'..edim then if jg[2] then gh('Saving ' .. jg[2] .. ', ' .. game.Players[qtrj].DisplayName) e(jg[2]) elseif not jg[2] then gh("\73\39\109\32\115\97\118\105\110\103\32\121\111\117\44\32" .. game.Players[qtrj].DisplayName) e(qtrj) end elseif jg[1] == 'di'..edim then game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[qtrj].Character.HumanoidRootPart.CFrame gh(di[math.random(1, #di)] .. game.Players[qtrj].DisplayName) elseif jg[1] == 'tp'..edim then ks.Character.HumanoidRootPart.CFrame = game.Players[jg[2]].Character.HumanoidRootPart.CFrame elseif jg[1] == 'reset'..edim then abort = true task.wait() t() abort = false elseif jg[1] == 'abort'..edim then abort = true task.wait() abort = false elseif jg[1] == 'rejoin'..edim then game:GetService("\84\101\108\101\112\111\114\116\83\101\114\118\105\99\101"):Teleport(game.PlaceId) elseif jg[1] == 'autostomp'..edim then p() elseif jg[1] == 'say'..edim then task.wait() gh(jg[2]) elseif jg[1] == 'drop'..edim then game:GetService("\82\101\112\108\105\99\97\116\101\100\83\116\111\114\97\103\101"):FindFirstChild("\46\103\103\47\117\110\116\105\116\108\101\100\104\111\111\100"):FireServer("\71\114\97\98\98\105\110\103",false) elseif jg[1] == 'i'..edim then gh(i[math.random(1, #i)] .. game.Players[qtrj].DisplayName) b(qtrj) elseif jg[1] == 'an'..edim then an() elseif jg[1] == 'add'..edim then if table.find(hosts, qtrj) then table.insert(users, jg[2]) r(jg[2]) else gh('lol mans thought he can add himself??') end end end) gh('Registered ' .. game.Players[qtrj].DisplayName) end end for _,qtrj in pairs(game.Players:GetPlayers()) do r(qtrj.Name) end game.Players.ChildAdded:Connect(function(qtrj) r(qtrj.Name) end) coroutine.resume(coroutine.create(function() while task.wait() do if players:FindFirstChild(ks) and ks.BodyEffects['K.O'].Value then t() end end end)) getgenv().loaded = true warn'loaded'
